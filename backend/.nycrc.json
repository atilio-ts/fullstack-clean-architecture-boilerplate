{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.d.ts",
    "src/**/*.test.ts",
    "src/server.ts",
    "tests/**/*",
    "coverage/**/*",
    "node_modules/**/*"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov"
  ],
  "cache": false,
  "instrument": true,
  "sourceMap": true,
  "produce-source-map": true
}